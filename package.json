{
  "name": "skedify-web-plugin-uri-encoding",
  "version": "0.0.0-development",
  "description": "Encoding of JSON object",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "eslint --cache src/**/*.js && rimraf lib",
    "lint": "eslint --cache src/**/*.js test/**/*.js",
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "prepublish": "npm test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "git-cz",
    "precommit": "npm run lint",
    "commitmsg": "node_modules/.bin/validate-commit-msg",
    "prepush": "npm test && npm run check-coverage",
    "postmerge": "npm install",
    "postrewrite": "npm install"
  },
  "homepage": "https://github.com/skedify/skedify-web-plugin-uri-encoding",
  "author": {
    "name": "Skedify",
    "email": "info@skedify.co",
    "url": "http://skedify.co"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skedify/skedify-web-plugin-uri-encoding.git"
  },
  "bugs": {
    "url": "https://github.com/skedify/skedify-web-plugin-uri-encoding/issues"
  },
  "keywords": [
    "skedify",
    "web-plugin",
    "uri-encoding"
  ],
  "files": [
    "lib"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "codecov.io": "^0.1.6",
    "commitizen": "^2.9.6",
    "cross-env": "^3.2.4",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-plugin-better": "^0.1.5",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-require-in-package": "^1.0.3",
    "husky": "^0.13.3",
    "nyc": "^10.3.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-node-resolve": "^2.1.1",
    "rollup-watch": "^3.2.2",
    "semantic-release": "^6.3.6",
    "validate-commit-msg": "^2.12.1"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "subjectPattern": "[a-z].+",
      "subjectPatternErrorMsg": "Subject is invalid",
      "helpMessage": "Use `npm run commit`. The subject should complete the following sentence: 'When applied, this commit will ...''"
    }
  }
}